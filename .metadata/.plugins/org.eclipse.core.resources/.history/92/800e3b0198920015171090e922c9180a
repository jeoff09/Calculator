# src/iia/iia_projectBundle/Resources/config/doctrine/Category.orm.yml
iia\iia_projectBundle\Entity\Category:
  type: entity
  table: category
  id:
    id:
      type: integer
      generator: { strategy: AUTO }
  fields:
    name:
      type: string
      length: 255
      unique: true
  oneToMany:
    jobs:
      targetEntity: Job
      mappedBy: category
  manyToMany:
     affiliates:
     targetEntity: Affiliate
      mappedBy: affiliate